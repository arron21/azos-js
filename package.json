{
  "--": "https://docs.npmjs.com/cli/v10/configuring-npm/package-json",
  "name": "@azist/monorepo",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "--source": "",
  "main": "out/azos-js.js",
  "description": "Azos Dev root monorepo package",
  "author": "Azist Group",
  "license": "MIT",
  "contributors": [
    {
      "name": "Dmitriy Khmaladze aka Zhabis, Ximik, itadapter, Rogognil",
      "email": "d@zhabis.com",
      "url": "http://zhabis.io"
    },
    {
      "name": "John Paul Kosh aka Devpro",
      "email": "devprohome@live.com",
      "url": "https://github.com/JohnPKosh"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azist/azos-js.git"
  },
  "keywords": [
    "azos",
    "unistack",
    "sky"
  ],
  "bugs": {
    "url": "https://github.com/azist/azos-js/issues"
  },
  "homepage": "https://github.com/azist/azos-js#readme",
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9.8.0"
  },
  "workspaces": [
    "packages/azos",
    "packages/azos-ui"
  ],
  "scripts": {
    "pack-local": "npm pack \"./packages/azos/\" --pack-destination=\"./out\" && npm pack \"./packages/azos-ui/\" --pack-destination=\"./out\"",
    "build": "npm run build --workspace=azos",
    "test": "npm run test --workspace=azos && npm run test --workspace=azos-ui",
    "test-azos": "npm run test --workspace=azos",
    "test-bro": "npm run test-bro --workspace=azos",
    "ui": "npm run ui --workspace=azos-ui",
    "test-ui": "npm run test --workspace=azos-ui"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "parcel": "^2.9.3"
  }
}
